<div class="menu-container">
  <div class="card-header col-md-10">
    <h2>
      &#127869 Current Menu
    </h2>
  </div>
  {{#each dishes }}
    <div class="dish col-md-5">
      <p>
        {{this.id}}.
        {{this.dish_name}}
      </p>
    </div>
    <div class="col-md-7">
      <p>
        Guest:
        {{this.guest_name}}
      </p>
    </div>
  {{/each}}
</div>